{
  "root": true,
  "overrides": [
    {
      "files": ["*.js"],
      "parser": "@babel/eslint-parser",
      "plugins": ["prettier"],
      "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
      ],
      "rules": {
        "no-console": "warn",
        "prettier/prettier": "error"
      }
    }
  ]
}
